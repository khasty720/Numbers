          <li><a><%= link_to 'Dogs', dogs_path %></a></li>
				</ul>
			</div>
			<div class="content">

        <!--Rendered Content -->
        <%= yield %>

			</div>
			<div class="footer">
				&copy; Copyright 2016
			</div>
		</div>



</body>
</html>
