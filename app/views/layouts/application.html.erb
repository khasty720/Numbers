				<h1 class="header-heading">MY SITE NAME</h1>
			</div>
			<div class="nav-bar">
				<ul class="nav">
          <li><a><%= link_to 'Index', root_path %></a></li>
					<li><a><%= link_to 'Home', home_path %></a></li>
					<li><a><%= link_to 'Users', users_path %></a></li>
